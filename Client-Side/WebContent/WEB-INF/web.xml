<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Pruebas</display-name>
  	<servlet>
        <servlet-name>Servlet Iniciar Sesion</servlet-name>
        <servlet-class>com.coronatickets.controllers.LoginHome</servlet-class>
  	</servlet>
  	<servlet-mapping>
        <servlet-name>Servlet Iniciar Sesion</servlet-name>
        <url-pattern>/login</url-pattern>
  	</servlet-mapping>
  	<servlet>
        <servlet-name>Servlet Login</servlet-name>
        <servlet-class>com.coronatickets.controllers.Login</servlet-class>
  	</servlet>
  	<servlet-mapping>
        <servlet-name>Servlet Login</servlet-name>
        <url-pattern>/iniciar-sesion</url-pattern>
  	</servlet-mapping>
	<servlet>
        <servlet-name>Casos de Prueba</servlet-name>
        <servlet-class>com.coronatickets.controllers.CasosPrueba</servlet-class>
  	</servlet>
  	<servlet-mapping>
        <servlet-name>Casos de Prueba</servlet-name>
        <url-pattern>/cargarcasosdeprueba</url-pattern>
  	</servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>